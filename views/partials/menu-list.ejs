<% if (menuItems.length === 0) { %>
    <p>No items found in this category.</p>
<% } else { %>
    <% menuItems.forEach(item => { %>
        <p>
            <% if (item.rating === 5) { %> ⭐ <% } %>
            <strong><%= item.name %></strong> – $<%= item.price %>
        </p>
        <p><em><%= item.details %></em></p>
        <p>Rating: <%= item.rating %>/5</p>
    <% }); %>
<% } %>